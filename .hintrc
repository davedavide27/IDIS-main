{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 125",
    "not chrome <= 126",
    "not edge <= 125",
    "not ios_saf <= 17.5",
    "not opera <= 110",
    "not safari <= 17.5",
    "not samsung <= 25"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off"
  }
}